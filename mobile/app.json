{
  "expo": {
    "name": "BookStore",
    "slug": "bookstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "bookstore",
    "icon": "./assets/logo.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ucmobileapk.bookstore"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b5d2bc8a-2cf0-43e8-af7f-1dd462a7d389"
      }
    },
    "owner": "ucmobileapk",
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b5d2bc8a-2cf0-43e8-af7f-1dd462a7d389"
    },
    "platforms": ["ios", "android"]
  }
}

